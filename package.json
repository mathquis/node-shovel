    "@koa/router": "^10.1.1",
    "amqplib": "^0.10.0",
    "cli-table": "^0.3.11",
    "clone": "^2.1.2",
    "colors": "^1.4.0",
    "convict": "^6.2.3",
    "cron": "^2.0.0",
